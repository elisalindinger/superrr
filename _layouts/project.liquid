<!doctype html>
<html lang="de">
<head>
    <title>{{ page.title }}</title>
    <meta charset="utf-8">
    <meta name="description" content="{{ page.description }}">
    <meta name="author" content="Rainbow Unicorn">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="{{ "/assets/main.css?v=2.5" | relative_url }}">
    {% include favicon-superrr.liquid %}
    {% include opengraph-superrr.liquid %}
</head>
<body>
<div class="loader">
    <span>SUPERRR.NET</span>
</div>
<div id="barba-wrapper">
    <div class="barba-container">
        {% include header.liquid page_type=page.header_type %}
        <div class="page-wrap lab col-xs-24">
            <div class="row">
                <div class="section col-xs-24 home">
                    {% include partials/navigation.liquid %}
                    <div class="content-wrap shrinked text-wrap image-block fade-in">
                        <img class="img-responsive" src="{{ site.baseurl }}{{ page.img }}"/>
                    </div>
                    <div class="content-wrap shrinked text-wrap content-block fade-in">
                        {{ content }}
                    </div>
                </div>
            </div>
            <!-- newsletter/social -->
            {% include partials/social.liquid page_type=page.header_type %}

        </div>
    </div>
</div>

{% include footer.liquid %}
